{
    "name": "postitief/piwik-tracking",
    "description": "Track pages using a twig function.",
    "type": "bolt-extension",
    "keywords": [
        "postitief", "piwik", "analytics", "piwik-tracking", "tracking", "dybo"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dybo",
            "email": "d.boertje@postitief.nl"
        }
    ],
    "minimum-stability": "1.0",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Postitief\\PiwikTracking\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Postitief\\PiwikTracking\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Postitief\\PiwikTracking\\Extension"
    }
}
